					</control>
				</focusedlayout>
			</control>
			<control type="spincontrol" id="513">
				<description>Next page button</description>
				<posx>650</posx>
				<posy>440</posy>
				<subtype>page</subtype>
				<font>font12</font>
				<onleft>503</onleft>
				<onright>503</onright>
				<ondown>503</ondown>
				<onup>503</onup>
