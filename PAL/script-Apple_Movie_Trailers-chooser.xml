					</control>
				</focusedlayout>
			</control>
			<control type="spincontrol" id="514">
				<description>Next page button</description>
				<posx>650</posx>
				<posy>440</posy>
				<subtype>page</subtype>
				<font>font12</font>
				<onleft>504</onleft>
				<onright>504</onright>
