<window id="2006">
	<defaultcontrol>2</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<controls>
		<control type="visualisation" id="2">
			<description>visualisation</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>720</width>
			<height>576</height>
		</control>
		<!-- codec & viz infos -->
		<control type="image">
			<description>preset background image</description>
			<posx>40</posx>
			<posy>40</posy>
			<width>640</width>
			<height>65</height>
			<colordiffuse>A1FFFFFF</colordiffuse>
			<texture>video-osd_menu_bg.png</texture>
			<visible>Player.ShowCodec</visible>
			<animation effect="fade" time="500">WindowOpen</animation>
			<animation effect="fade" time="500">VisibleChange</animation>
		</control>
		<control type="label">
			<description>Codecinfo</description>
			<width>600</width>
			<posx>75</posx>
			<posy>50</posy>
			<label>Audio | $INFO[musicplayer.Codec,Codec: ,]$INFO[musicplayer.Bitrate, - Bitrate: ,kbps]$INFO[musicplayer.bitspersample, - Bits:,bit]$INFO[musicplayer.Samplerate, - Freq.: ,kHz] $INFO[musicplayer.Channels, - Channels: ]</label>
			<align>left</align>
			<font>font12</font>
			<visible>Player.ShowCodec</visible>
			<animation effect="fade" time="500">WindowOpen</animation>
			<animation effect="fade" time="500">VisibleChange</animation>
		</control>
		<control type="label">
			<description>Presets label</description>
			<posx>75</posx>
			<posy>75</posy>
			<width>560</width>
			<label>Visuals | fps: $INFO[System.FPS] - </label>
			<font>font12</font>
			<align>left</align>
			<visible>Player.ShowCodec</visible>
			<animation effect="fade" time="500">WindowOpen</animation>
			<animation effect="fade" time="500">VisibleChange</animation>
		</control>
		<control type="label">
			<description>Presets label</description>
			<posx>225</posx>
			<posy>75</posy>
			<width>420</width>
			<label>$INFO[Visualisation.Name]$INFO[Visualisation.Preset, - ]</label>
			<font>font12</font>
			<align>left</align>
			<visible>Player.ShowCodec</visible>
			<animation effect="fade" time="500">WindowOpen</animation>
			<animation effect="fade" time="500">VisibleChange</animation>
		</control>
		<control type="image">
			<description>Preset locked image</description>
			<posx>210</posx>
			<posy>76</posy>
			<width>12</width>
			<height>15</height>
			<visible>Visualisation.Locked + Player.ShowCodec</visible>
			<animation effect="fade" time="500">WindowOpen</animation>
			<animation effect="fade" time="500">VisibleChange</animation>
			<texture>arenaitem-private.png</texture>
		</control>
		<!-- media infos -->
		<control type="image">
			<description>media info background image</description>
			<posx>-10</posx>
			<posy>455</posy>
			<width>740</width>
			<height>150</height>
			<visible>Player.ShowInfo</visible>
			<animation effect="fade" time="500">VisibleChange</animation>
			<texture>video-osd-backgroundshadow.png</texture>
		</control>
		<control type="image">
			<description>Track Info Panel</description>
			<posx>40</posx>
			<posy>423</posy>
			<width>640</width>
			<height>115</height>
			<colordiffuse>A1FFFFFF</colordiffuse>
			<visible>Player.ShowInfo</visible>
			<animation effect="fade" time="500">WindowOpen</animation>
			<animation effect="fade" time="500">VisibleChange</animation>
			<texture>video-osd_menu_bg.png</texture>
		</control>
		<control type="image">
			<description>Jewel CD Case Back</description>
			<posx>108</posx>
			<posy>393</posy>
			<width>115</width>
			<height>156</height>
			<visible>Player.ShowInfo</visible>
			<texture>music-overlay-caseinside.png</texture>
			<animation effect="fade" time="500">WindowOpen</animation>
			<animation effect="fade" time="500">VisibleChange</animation>
		</control>
		<control type="image">
			<description>music logo</description>
			<posx>108</posx>
			<posy>393</posy>
			<width>115</width>
			<height>156</height>
			<visible>Player.ShowInfo</visible>
			<info>MusicPlayer.Cover</info>
			<animation effect="fade" time="500">WindowOpen</animation>
			<animation effect="fade" time="500">VisibleChange</animation>
		</control>
		<control type="image">
			<description>Jewel CD Case</description>
			<posx>85</posx>
			<posy>385</posy>
			<width>147</width>
			<height>179</height>
			<visible>Player.ShowInfo</visible>
			<texture>music-overlay-cdcase.png</texture>
			<animation effect="fade" time="500">WindowOpen</animation>
			<animation effect="fade" time="500">VisibleChange</animation>
		</control>
		<control type="fadelabel">
			<description>Artist label</description>
			<width>390</width>
			<posx>260</posx>
			<posy>430</posy>
			<info>MusicPlayer.Artist</info>
			<visible>Player.ShowInfo</visible>
			<animation effect="fade" time="500">WindowOpen</animation>
			<animation effect="fade" time="500">VisibleChange</animation>
			<align>left</align>
			<font>font14</font>
		</control>
		<control type="fadelabel">
			<description>Title label</description>
			<width>390</width>
			<posx>260</posx>
			<posy>455</posy>
			<info>MusicPlayer.Title</info>
			<visible>Player.ShowInfo</visible>
			<animation effect="fade" time="500">WindowOpen</animation>
			<animation effect="fade" time="500">VisibleChange</animation>
			<align>left</align>
			<font>font16</font>
		</control>
		<control type="fadelabel">
			<description>Album label</description>
			<width>390</width>
			<posx>260</posx>
			<posy>483</posy>
			<info>MusicPlayer.Album</info>
			<visible>Player.ShowInfo</visible>
			<animation effect="fade" time="500">WindowOpen</animation>
			<animation effect="fade" time="500">VisibleChange</animation>
			<align>left</align>
			<font>font12</font>
		</control>
		<control type="label">
			<description>time label</description>
			<width>300</width>
			<posx>260</posx>
			<posy>505</posy>
			<label>$INFO[musicplayer.Time]$INFO[musicplayer.Duration, / ]</label>
			<visible>Player.ShowInfo</visible>
			<animation effect="fade" time="500">WindowOpen</animation>
			<animation effect="fade" time="500">VisibleChange</animation>
			<align>left</align>
			<font>font12</font>
		</control>
		<control type="label">
			<description>Playlist</description>
			<width>200</width>
			<posx>640</posx>
			<posy>505</posy>
			<label>$INFO[musicplayer.Playlistposition,$LOCALIZE[435] ]$INFO[musicplayer.Playlistlength, / ]</label>
			<visible>Player.ShowInfo</visible>
			<animation effect="fade" time="500">WindowOpen</animation>
			<animation effect="fade" time="500">VisibleChange</animation>
			<align>right</align>
			<font>font12</font>
		</control>
	</controls>
</window>